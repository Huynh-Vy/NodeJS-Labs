
<%- include("partials/header") -%>

<h1><%= pageTitle %></h1>
<p><%= homeContent %></p>
<% postContent.forEach(function(post) { %>
  <h3><%= post.title  %></h3>
  <p>
    <%= post.content.substring(0,100) + "..." %>
    <a href="/posts/<%= post._id%>">Read More</a>

  </p>

<% }); %>


<%- include("partials/footer") -%>
